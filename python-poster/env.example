# 环境配置示例文件
# 复制此文件为 .env 并修改相应配置

# Flask配置
FLASK_ENV=development
FLASK_DEBUG=True
SECRET_KEY=your-secret-key-here

# 服务配置
HOST=0.0.0.0
PORT=5001

# 文件存储配置
UPLOAD_FOLDER=uploads
TEMP_FOLDER=temp
STATIC_FOLDER=static

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=app.log

# 数据库配置（如果使用）
DATABASE_URL=sqlite:///app.db
